{
  "id" : "@artifactId@-@version@",
  "name" : "ldp module",
  "provides" : [ {
    "id" : "db-logs",
    "version" : "1.0",
      "handlers": [ {
      "methods": [ "GET" ],
      "pathPattern" : "/ldp/db/log",
      "permissionsRequired": [ "ldp.db.log.get" ]
    } ]
  } ],
  "permissionSets": [{
    "description": "Get ldp system logs from the database",
    "displayName": "LDP -- Get System Logs",
    "permissionName": "ldp.db.log.get"
  }],
  "launchDescriptor" : {
    "dockerImage": "@artifactId@:@version@",
    "dockerPull": false,
    "dockerArgs": {
      "HostConfig": {
        "Memory": 357913941,
        "PortBindings": { "8001/tcp": [ { "HostPort": "%p" } ] }
      }
    },
    "env": [
      { "name": "JAVA_OPTIONS",
        "value": "-XX:MaxRAMPercentage=66.0"
      }
    ]
  }
}
